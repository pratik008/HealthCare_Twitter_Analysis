               <code code="50570-1" codeSystem="2.16.840.1.113883.6.1" displayName="OTC - DO NOT USE SECTION"/>
               <title>
                  <content styleCode="italics">Do not use</content>
               </title>
               <text>
                  <list listType="unordered">
                     <item>in the eyes</item>
                     <item>if you are allergic to any of the ingredients</item>
                  </list>
               </text>
