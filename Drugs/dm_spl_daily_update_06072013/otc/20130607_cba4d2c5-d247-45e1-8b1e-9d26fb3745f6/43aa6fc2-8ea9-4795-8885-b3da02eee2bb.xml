               <code code="34067-9" codeSystem="2.16.840.1.113883.6.1" displayName="INDICATIONS &amp; USAGE SECTION"/>
               <text>
                  <paragraph>
                     <content styleCode="bold">Directions</content>
                  </paragraph>
                  <list listType="unordered" ID="i61b23bd2-064c-4e5c-8e2b-5a8839119e7a">
                     <item>Non-Water Resistant Product.<br/>
                     </item>
                     <item>Apply liberally 15 minutes before sun exposure. <br/>
                     </item>
                     <item>Reapply at least every 2 hours. <br/>
                     </item>
                     <item>Children under 6 months : Ask a doctor<br/>
                     </item>
                  </list>
               </text>
               <effectiveTime value="20130606"/>
            </section>
