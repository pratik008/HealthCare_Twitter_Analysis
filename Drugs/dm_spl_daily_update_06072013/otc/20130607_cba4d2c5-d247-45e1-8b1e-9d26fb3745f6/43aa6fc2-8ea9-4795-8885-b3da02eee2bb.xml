               <text>
                  <paragraph>
                     <content styleCode="bold">Dosage and Administration</content>
                  </paragraph>
                  <list listType="unordered" ID="icd9c7634-a141-41e3-86cd-18d1dd6ddbf0">
                     <item>Apply liberally 15 minutes before sun exposure. </item>
                     <item>Reapply at least every 2 hours.</item>
                     <item>Children under 6 months : Ask a doctor</item>
                  </list>
               </text>
               <effectiveTime value="20130606"/>
            </section>
