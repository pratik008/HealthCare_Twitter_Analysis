         <component>
            <section>
               <id root="1f1a69e7-502d-41c7-85d1-dd6bd4eef026"/>
               <code code="34071-1" codeSystem="2.16.840.1.113883.6.1" displayName="WARNINGS SECTION"/>
               <text>
                  <paragraph>
                     <content styleCode="bold">Warnings:</content>
                  </paragraph>
                  <paragraph>This product has been shown only to prevent sunburn NOT skin cancer or early skin aging.</paragraph>
                  <paragraph>For external use only. Keep out of reach of children. If swallowed, get medical help or contact a Poison Control Center right away.</paragraph>
                  <paragraph/>
               </text>
               <effectiveTime value="20130606"/>
            </section>
         </component>
